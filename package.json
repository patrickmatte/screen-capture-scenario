{
	"name": "screen-capture-pro",
	"version": "0.0.1",
	"author": "Patrick Matte <patrick.mattek@gmail.com>",
	"description": "Build script that uses webpack-dev-server to compile twig templates to static html, using Sass for CSS.",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack-dev-server --progress --open --history-api-fallback",
		"build": "cross-env NODE_ENV=production webpack -p --progress --config webpack.config.js"
	},
	"browserslist": "> 1%, last 2 versions, IE 11",
	"babel": {
		"sourceMaps": false,
		"presets": [
			"@babel/preset-env"
		]
	},
	"repository": {
		"type": "git",
		"url": "ssh://git@git.firstborn.com:8443/scm/dew/dew-ice-2018.git"
	},
	"devDependencies": {
		"@babel/core": "^7.7.2",
		"@babel/preset-env": "^7.7.1",
		"babel-loader": "^8.0.6",
		"copy-webpack-plugin": "^5.0.4",
		"cross-env": "^6.0.3",
		"css-loader": "^3.2.0",
		"dotenv-webpack": "^1.7.0",
		"file-loader": "^4.2.0",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.8.0",
		"node-sass": "^4.13.0",
		"raw-loader": "^0.5.1",
		"recursive-fs": "^1.1.2",
		"sass-loader": "^8.0.0",
		"sass-resources-loader": "^2.0.1",
		"source-map-loader": "^0.2.4",
		"style-loader": "^1.0.0",
		"terser-webpack-plugin": "^2.2.1",
		"url-loader": "^2.2.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.9.0"
	},
	"dependencies": {
		"mustache": "^2.3.0",
		"three": "^0.109.0"
	}
}
